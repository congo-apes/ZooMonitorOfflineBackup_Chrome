{
  "manifest_version": 3,
  "name": "Local Storage Saver",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "scripting",
    "downloads",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "64": "icon64.png",
      "128": "icon128.png"
    }
  }
}